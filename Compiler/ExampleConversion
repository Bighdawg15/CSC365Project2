//////////////////////////////////////////////////////////////////////////////////
HLC Example:
//////////////////////////////////////////////////////////////////////////////////

unsigned a, b, c;
signed x, y, z;    //move into stack, but this would have a flag of somesort : Check with cmp

a = 3;
b = 15 + a;  
c = (b * a) / 10;

x = -5;
y = 13;


if c <= 10
	x = y + 10
else
	x = y - 20

while y > 0
	print y
	print \n
	print x
	print \n
	y = y - 1

