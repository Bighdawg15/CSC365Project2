Example Code:
--------------------------------------------------------------
unsigned a b c
signed x y z

a = 3
b = 15 + a
c = b * a / 10

x = -5
y = 13

if c <= 10
	x = y + 10
else
	x = y - 20

while y > 0
	print y
	print \n
	print x
	print \n
	y = y - 1
------------------------------------------------------------
Conversions from Assembly to Machine Code:
------------------------------------------------------------


                Assembly                |   Machine Code    
-----------------------------------------------------------------------------
mov ecx, eax                            |                   
-----------------------------------------------------------------------------
add ecx, eax, ebx                       |                  
-----------------------------------------------------------------------------
sub ecx, eax, ebx                       |                   
-----------------------------------------------------------------------------
mult ecx, eax, ebx                      |                 
-----------------------------------------------------------------------------
div ecx, eax, ebx                       |                  
-----------------------------------------------------------------------------
addsub eax, eax, ebx, ecx               |                   
-----------------------------------------------------------------------------
subadd eax, eax, ebx, ecx               |                   
-----------------------------------------------------------------------------
multidiv eax, eax, ebx, ecx             |                  
-----------------------------------------------------------------------------
divmulti eax, eax, ebx, ecx             |                   
-----------------------------------------------------------------------------
cmp x,y                                 |                   
-----------------------------------------------------------------------------
jg                                      |                   
-----------------------------------------------------------------------------
jge                                     |                   
-----------------------------------------------------------------------------
jl                                      |                  
-----------------------------------------------------------------------------
jle                                     |                   
-----------------------------------------------------------------------------
jne                                     |                   
-----------------------------------------------------------------------------
je                                      |                   
-----------------------------------------------------------------------------
jmp                                     |                   
-----------------------------------------------------------------------------








